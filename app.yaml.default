runtime: go
api_version: go1.9

handlers:


# Serve static files
- url: /static
  static_dir: static

# All URLs are handled by the Mux router
- url: /.*
  script: _drieshooghe
  secure: always

skip_files:
- ^(.*/)?#.*#$
- ^(.*/)?.*~$
- ^(.*/)?.*\.py[co]$
- ^(.*/)?.*/RCS/.*$
- ^(.*/)?\..*$
- ^(.*/)?.*\.bak$
- node_modules/

env_variables:
  GITHUB_OAUTH_TOKEN: ''